<h4>Links inside card [autogrid]</h4>
<section class="md--auto-grid md--auto-grid--third">

	<div class="auto-grid__cell">
		<article class="flex-col h-full shadow group">
			<div class="bg-cover ubc-card__media min-h-64"
				style="background-image: url(https://live.staticflickr.com/4584/38588637591_0241df7a93_z_d.jpg);"></div>
			<div class="ubc-card__content">
				<h2 class="card__title"><a href="#" class="no-underline link-expand">{{ card1Title }}</a></h2>
				<p>Whole card link. {{ card1Text }}</p>
			</div>

			<div class="text-right ubc-card__actions">
				<div class="inline-block ubc-card__action-buttons">
					<p class="unit-button unit-button--light group-hover--bg-ubc-blue-cobalt group-hover--text-white"> {{ linkText }}</p>
				</div>
			</div>
		</article>
	</div>

	<div class="auto-grid__cell">
		<div class="ubc-card ubc-card--vert ubc-card--shadow">
		<a href="#" class="flex-1 text-black no-underline ubc-card__primary-action">
			<div class="bg-cover ubc-card__media min-h-64"
				style="background-image: url(https://live.staticflickr.com/4584/38588637591_0241df7a93_z_d.jpg);"></div>
			<div class="ubc-card__content">
				<h2 class="card__title">{{ card1Title }}</h2>
				<p>Split Links. {{ card1Text }}</p>
			</div>

		</a>
		<div class="text-right ubc-card__actions">
			<div class="inline-block ubc-card__action-buttons">
				<p>
					<a href="#" class="unit-button unit-button--mid">Different Link</a>
				</p>
			</div>
		</div>
	</div>
	</div>

	<div class="auto-grid__cell">
		<div class="ubc-card ubc-card--vert ubc-card--shadow">
			<div class="block text-white bg-cover ubc-card__media min-h-64 has-content--after has-gradient-bottom--after"
				style="background-image: url(https://live.staticflickr.com/4584/38588637591_0241df7a93_z_d.jpg);">
				<div class="relative z-30 flex flex-wrap items-end p-4 ubc-card__media-content min-h-64">
					<div class="ubc-card__primary">
						<h2 class="text-white card__title">{{ card1Title }}</h2>
						<h3 class="card__subtitle">{{ cardAttribution }}</h3>
					</div>
				</div>
			</div>
			<div class="ubc-card__content">No link before footer. {{ card1Text }}</div>
			<div class="border-t ubc-card__actions">
				<div class="inline-block ubc-card__action-buttons">
					<a href="#" class="unit-button unit-button--dark">Read</a>
					<button class="unit-button unit-button--light">Bookmark</button>
				</div>
			</div>
		</div>
	</div>

</section>

<hr />

