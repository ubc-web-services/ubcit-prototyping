{# CONFIG #}
{% set unitName = "UBC Web Services" %}
{% set siteURL = "it.ubc.ca" %}
{% set facultyName = "" %}
{% set campus = '' %}
{% set clfversion = 7 %}
{% set clf_sticky = TRUE %}
{% set containerClasses = 'container px-5 py-12 mx-auto md--px-4' %}
{% set pageIntro = '<p class="text-lead">Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p>' %}
{% set loremIpsum = '<p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.</p>' %}

<!doctype html>
<html class="no-js" lang="en">

	<head>
		<meta charset="UTF-8">
		<title>{{ title }}
			|
			{{ unitName }}</title>
		{% include "../includes/head.twig" %}
		{% block custom_css %}{% endblock %}
	</head>

	<body class="js-bootstrap">
		<div id="main-content"> {% block masthead %}{% endblock %}
			{% if showTitle %}
				<h1 class="mt-0 page-title text-ubc-blue">
					{{ pageTitle }}
					</h1>
			{% endif %}
			{% block content %}{% endblock %}
			{% block areab %}{% endblock %}
			{% block areac %}{% endblock %}
			{% block aread %}{% endblock %}
			{% block prefooter %}{% endblock %}
		</div>
		{% include "../includes/scripts.twig" %}
		{% block custom_javascript %}{% endblock %}
	</body>

</html>
