{
  "name": "ubc-prototypng",
  "version": "2.0.1",
  "description": "UBC Prototyping scaffolding",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubc-web-services/ubcit-prototyping.git"
  },
  "keywords": [
    "clf",
    "ubc"
  ],
  "author": "James O'Toole, UBC IT, Web Services",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ubc-web-services/ubcit-prototyping/issues"
  },
  "homepage": "https://github.com/ubc-web-services/ubcit-prototyping#readme",
  "engines": {
    "yarn": ">= 1.6",
    "node": ">= 8.11"
  },
  "scripts": {
    "serve": "live-server ./build --watch",
    "css": "TAILWIND_MODE=build NODE_ENV=production postcss ./src/css/*.css -d ./build/css --ext css",
    "css-lint": "npm run lint:css",
    "lint:css": "stylelint \"src/**/*.css\" || exit 0",
    "css-fix": "npm run lint:fix-css",
    "lint:fix-css": "stylelint \"src/**/*.css\" --fix || exit 0",
    "html": "npm run build:html",
    "build:html": "NODE_ENV=production twig-cli \"src/html/*.html.twig\" --base src/html/ --output build/ && ncp src/html/img build/img",
    "prod": "npm run production",
    "build:prod": "npm run production",
    "production": "TAILWIND_MODE=build NODE_ENV=production postcss ./src/css/*.css -d ./build/css --ext css && webpack --config webpack.prod.js",
    "build": "npm run prod && npm run html"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "tiny-slider": "^2.9.4",
    "twig-cli-tmpl": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@popperjs/core": "^2.11.8",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "babel-preset-minify": "^0.5.2",
    "clickout-event": "^1.1.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "live-server": "^1.2.1",
    "ncp": "^2.0.0",
    "postcss": "^8.4.47",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-simple-vars": "^7.0.1",
    "stylelint": "^15.11.0",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-no-browser-hacks": "^1.2.1",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.4.13",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  },
  "browserslist": [
    "defaults"
  ]
}
